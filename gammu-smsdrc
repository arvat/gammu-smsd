OutboxPath = /var/log/gammu/outbox/
SentSMSPath = /var/log/gammu/sent/
ErrorSMSPath = /var/log/gammu/error/
DebugLevel = 1
RunOnReceive = /root/receive_sms.sh
CheckBattery = 0
